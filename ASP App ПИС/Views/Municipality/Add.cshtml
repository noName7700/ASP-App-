

<div class="content-wrapper">
    <!-- START PAGE CONTENT-->
    <div class="page-heading">
        <h1 class="page-title">Добавить муниципалитет</h1>
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="index.html"><i class="la la-home font-20"></i></a>
            </li>
            <li class="breadcrumb-item">
                <a href="/municipality/">Муниципалитеты</a>
            </li>
            <li class="breadcrumb-item">Добавить муниципалитет</li>
        </ol>
    </div>
    <div class="page-content fade-in-up">
        <div class="ibox">
            <div class="ibox-head">
                <div class="ibox-title">Новый муниципалитет</div>
            </div>
            @if (ViewData["err"] != null)
            {
                <div class="alert alert-danger alert-dismissable fade show">
                    <button class="close" data-dismiss="alert" aria-label="Close">×</button>
                    <strong>Ошибка!</strong> @ViewData["err"]
                </div>
            }
            <div class="ibox-body">
                <form class="form-horizontal" method="post">
                    <div class="form-group row has-feedback">
                        <label class="col-sm-2 col-form-label" for="namemun">Наименование:</label>
                        <div class="col-sm-8">
                            <input class="form-control" id="namemun" type="text" name="name" required>
                            <span class="glyphicon form-control-feedback" aria-hidden="true"></span>
                            <span class="help-block">
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-10 ml-sm-auto">
                        <button type="submit" class="btn btn-info">Добавить</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- END PAGE CONTENT-->
</div> 